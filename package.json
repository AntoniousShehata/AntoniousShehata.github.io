{
  "name": "create-tailwind-app",
  "version": "1.0.1",
  "description": "Build the HTML template with tailwind css.",
  "type": "commonjs",
  "scripts": {
    "start": "live-server",
    "build": "postcss css/main.css -o assets/css/build/tailwind.css --minify",
    "tailwind": "tailwindcss  -i css/main.css -o public/assets/css/build/tailwind.css --minify  & live-server ./public ",
    "dev": " live-server ./public & postcss css/main.css -o public/assets/css/build/tailwind.css --minify --watch  "
  },
  "keywords": [
    "HTML template",
    "tailwindcss",
    "tailwind",
    "HTML",
    " css framework"
  ],
  "author": "Rajdeep Singh",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "live-server": "^1.2.2",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "react-i18next": "^15.4.1"
  }
}
